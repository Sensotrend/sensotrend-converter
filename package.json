{
  "name": "sensotrend-converter",
  "version": "1.0.26-beta",
  "main": "src/index.mjs",
  "keywords": [
    "FHIR",
    "Nightscout",
    "Tidepool",
    "HL7",
    "Sensotrend",
    "LibreCsv"
  ],
  "license": "BSD-2-Clause",
  "description": "Data format converter for managing various JSON data formats related to type 1 diabetes",
  "repository": "https://github.com/sensotrend/sensotrend-converter",
  "dependencies": {
    "fs-extra": "^9.1.0",
    "moment": "^2.29.1",
    "stjs": "0.0.5",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/eslint-plugin": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-private-methods": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "chai": "^4.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.6",
    "minami": "^1.2.3",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.3",
    "unzipper": "^0.10.11"
  },
  "nyc": {
    "all": false,
    "sourceMap": false,
    "instrument": true,
    "cache": false
  },
  "engines": {
    "node": ">= 14.15.2",
    "npm": ">= 7.5.4"
  },
  "scripts": {
    "docs": "rm -rf ./docs && jsdoc --configure .jsdoc.json",
    "test": "mocha --exit --timeout 8000 tests/*.test.mjs",
    "test2": "npm run unzip && mocha --exit --timeout 199000 tests/librecsv.test.mjs",
    "coverage": "nyc mocha --exit --timeout 8000 tests/*.test.mjs",
    "coverage-html": "rm -rf .nyc_output && npm run-script coverage && nyc report --reporter html && nyc report --reporter text-lcov > coverage/codecov.log",
    "unzip": "node ./tools/unZipFile.mjs"
  },
  "jshintConfig": {
    "esversion": 9
  }
}
