{
  "name": "sensotrend-converter",
  "version": "2.0.1-beta",
  "main": "src/index.mjs",
  "keywords": [
    "FHIR",
    "Nightscout",
    "Tidepool",
    "HL7",
    "Sensotrend",
    "LibreCsv"
  ],
  "license": "BSD-2-Clause",
  "description": "Data format converter for managing various JSON data formats related to type 1 diabetes",
  "repository": "https://github.com/sensotrend/sensotrend-converter",
  "dependencies": {
    "fs-extra": "^10.0.0",
    "moment": "^2.29.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "lodash": "^4.17.21"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.15.4",
    "@babel/eslint-parser": "^7.15.4",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/preset-env": "^7.15.4",
    "chai": "^4.3.4",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.7",
    "minami": "^1.2.3",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.6",
    "unzipper": "^0.10.11"
  },
  "nyc": {
    "all": false,
    "sourceMap": false,
    "instrument": true,
    "cache": false
  },
  "engines": {
    "node": ">= 14.15.2",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "docs": "rm -rf ./docs && jsdoc --configure .jsdoc.json",
    "test": "mocha --exit --timeout 18000 tests/*.test.mjs",
    "test_librecsv_convert": "mocha --exit --timeout 18000 tests/librecsv.test.mjs",
    "coverage": "nyc mocha --exit --timeout 8000 tests/*.test.mjs",
    "coverage-html": "rm -rf .nyc_output && npm run-script coverage && nyc report --reporter html && nyc report --reporter text-lcov > coverage/codecov.log",
    "unzip": "node ./tools/unZipFile.mjs"
  },
  "jshintConfig": {
    "esversion": 9
  }
}
